<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/legal_information" android:key="legal_information"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceScreen android:title="@string/huawei_privacy_policy_title" android:key="huawei_privacy_policy" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="com.android.settings.HuaweiPrivacyPolicyActivity" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/device_permitted_agreement" android:key="huawei_copyright" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="android.settings.HUAWEI_COPYRIGHT" />
    </PreferenceScreen>
    <Preference android:title="@string/settings_safetylegal_title" android:key="safety_info" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="android.settings.SAFETY_INFO_SETTINGS">
            <extra android:name="file_name" android:value="instance_si_phone_internet.html" />
            <extra android:name="android.intent.extra.TITLE" android:value="@string/settings_safetylegal_title" />
        </intent>
    </Preference>
    <PreferenceScreen android:title="@string/copyright_title" android:key="copyright" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="android.settings.COPYRIGHT" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/license_title" android:key="license" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="android.settings.LICENSE" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/terms_title" android:key="terms" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="android.settings.TERMS" />
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/webview_license_title" android:key="webview_license" android:widgetLayout="@layout/preference_widget_arrow">
        <intent android:action="android.settings.WEBVIEW_LICENSE" />
    </PreferenceScreen>
</PreferenceScreen>