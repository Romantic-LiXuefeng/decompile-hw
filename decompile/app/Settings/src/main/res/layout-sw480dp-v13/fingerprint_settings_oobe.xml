<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:gravity="center_horizontal" android:layout_gravity="center_horizontal" android:orientation="vertical" android:id="@id/logo_view" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <FrameLayout android:id="@id/logo_container" android:layout_width="@dimen/fp_oobe_logo_container_size" android:layout_height="@dimen/fp_oobe_logo_container_size">
            <ImageView android:layout_gravity="center" android:id="@id/finger_background" android:background="@drawable/bg_ball_fingerprint_oobe" android:layout_width="wrap_content" android:layout_height="wrap_content" />
            <ImageView android:layout_gravity="center_horizontal" android:id="@id/fingerprint_white" android:background="@drawable/ic_fingerprint_white" android:layout_width="@dimen/fp_oobe_logo_finger_white_size" android:layout_height="@dimen/fp_oobe_logo_finger_white_size" android:layout_marginTop="@dimen/oobe_logo_margin_top" />
        </FrameLayout>
        <TextView android:textSize="@dimen/fp_oobe_main_title_size" android:layout_gravity="center_horizontal" android:id="@id/func_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:fontFamily="HwChinese-medium" android:layout_marginStart="24.0dip" android:layout_marginEnd="24.0dip" />
        <TextView android:gravity="center_horizontal" android:id="@id/func_desc" android:layout_width="wrap_content" android:layout_marginTop="@dimen/fp_oobe_pad_func_desc_margin_top" style="@style/fpStartupDescStyle" />
    </LinearLayout>
    <ScrollView android:id="@id/desc_container" android:scrollbars="none" android:layout_width="fill_parent" android:layout_height="0.0dip" android:layout_weight="1.0" />
    <FrameLayout android:id="@id/button_bar_layout" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <include layout="@layout/button_bar" />
    </FrameLayout>
</LinearLayout>