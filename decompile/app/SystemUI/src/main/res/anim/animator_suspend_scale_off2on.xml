<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:interpolator="@interpolator/cubic_bezier_interpolator_type_20_90" android:ordering="sequentially"
    <set android:interpolator="@interpolator/cubic_bezier_interpolator_type_20_90" android:ordering="together">
        <objectAnimator android:duration="100" android:valueFrom="1.0" android:valueTo="1.8" android:propertyName="scaleX" />
        <objectAnimator android:duration="100" android:valueFrom="1.0" android:valueTo="1.8" android:propertyName="scaleY" />
    </set>
    <set android:interpolator="@interpolator/cubic_bezier_interpolator_type_20_90" android:ordering="together">
        <objectAnimator android:duration="100" android:valueFrom="1.8" android:valueTo="0.3" android:propertyName="scaleX" />
        <objectAnimator android:duration="100" android:valueFrom="1.8" android:valueTo="0.3" android:propertyName="scaleY" />
    </set>
    <set android:interpolator="@interpolator/cubic_bezier_interpolator_type_20_90" android:ordering="together">
        <objectAnimator android:duration="150" android:valueFrom="0.3" android:valueTo="1.0" android:propertyName="scaleX" />
        <objectAnimator android:duration="150" android:valueFrom="0.3" android:valueTo="1.0" android:propertyName="scaleY" />
    </set>
</set>
