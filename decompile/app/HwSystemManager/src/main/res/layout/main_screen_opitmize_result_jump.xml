<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/viewgroup" android:layout_width="match_parent" android:layout_height="wrap_content">
    <View android:id="@+id/empty_line" style="@style/divider_line" />
    <RelativeLayout android:id="@+id/layout_main" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/layout_main_margin_top" android:layout_marginBottom="@dimen/layout_main_margin_bottom" android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
        <RelativeLayout android:id="@+id/layout_process_opt" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true" android:layout_marginStart="8dp" android:layout_alignParentEnd="true">
            <TextView android:textSize="13sp" android:textColor="@color/prohibit_color" android:id="@+id/jumpinfo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" />
            <ImageView android:id="@+id/jumpicon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/ic_public_arrow" android:contentDescription="@string/confirm" android:layout_marginStart="8dp" android:layout_toEndOf="@+id/jumpinfo" />
            <ImageView android:id="@+id/complete_icon" android:visibility="gone" android:layout_width="UNKNOWN_DATA_0x1001" android:layout_height="UNKNOWN_DATA_0x1001" android:src="@drawable/ic_public_ok" android:layout_centerVertical="true" android:contentDescription="@string/confirm" android:layout_marginEnd="8dp" />
        </RelativeLayout>
        <RelativeLayout android:id="@+id/layout_des" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" android:layout_toStartOf="@+id/layout_process_opt" android:layout_alignParentStart="true"
            <RelativeLayout android:id="@+id/layout_name" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:textSize="15sp" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" />
            </RelativeLayout>
            <TextView android:textSize="13sp" android:textColor="@color/emui_list_secondray_text" android:id="@+id/description" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:layout_below="@+id/layout_name" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
