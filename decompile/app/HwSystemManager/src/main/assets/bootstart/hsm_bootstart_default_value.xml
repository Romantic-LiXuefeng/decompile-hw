    <!--
            配置文件作用:
                手机管家开机启动默认开关值设置。
            使用场景:
                手机管家开机启动中，默认值是禁止开机启动。如果产品需要指定某个应用的默认值，可以在这个文件中进行配置。
            加入这个名单后:
    allow="true"表示第一次开机后，默认允许这个应用开机启动。allow="false"表示禁止这个应用开机启动。
    -->
    <!-- package name="com.lbe.security" allow="true"/ -->
    <!-- package name="com.tencent.qqlive" allow="false"/ -->
    <package name="com.huawei.health" allow="true" />
    <package name="com.iflytek.inputmethod" allow="true" />
</resources>

