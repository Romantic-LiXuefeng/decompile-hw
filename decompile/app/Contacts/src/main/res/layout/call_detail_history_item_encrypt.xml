<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="@drawable/list_selector_call_history" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/call_log_list_item_height">
    <ImageView android:id="@+id/call_record_button" android:paddingBottom="16dp" android:visibility="gone" android:layout_width="UNKNOWN_DATA_0x7f0d00bf" android:layout_height="UNKNOWN_DATA_0x7f0d00bf" android:src="@drawable/ic_play" android:scaleType="centerInside" android:layout_alignParentTop="true" android:contentDescription="@string/call_record_description" android:paddingEnd="6dp" android:layout_alignParentEnd="true" />
    <ImageView android:id="@+id/play_voicemail_button" android:visibility="gone" android:layout_width="UNKNOWN_DATA_0x7f0d00bf" android:layout_height="UNKNOWN_DATA_0x7f0d00bf" android:src="@drawable/ic_play" android:scaleType="centerInside" android:layout_centerVertical="true" android:contentDescription="@string/call_record_description" android:paddingEnd="6dp" android:layout_alignParentEnd="true" />
    <RelativeLayout android:orientation="vertical" android:id="@+id/contact_date_first" android:paddingTop="@dimen/contact_list_item_padding_top" android:paddingBottom="@dimen/contact_list_item_padding_bottom" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_centerInParent="true" android:paddingStart="@dimen/call_log_indent_margin_end" android:paddingEnd="@dimen/call_log_indent_margin_end">
        <TextView android:id="@+id/contact_date" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/contact_calllog_detail_main_text_style" />
        <RelativeLayout android:id="@+id/outgoing_call_detail" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/contact_date"
            <RelativeLayout android:id="@+id/call_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true">
                <view android:id="@+id/call_type_icon" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:contentDescription="@string/image_content_description" android:layout_alignParentStart="true" class="com.android.contacts.calllog.CallTypeIconsView" />
                <ImageView android:id="@+id/call_type_image" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/stat_sys_sim1" android:scaleType="fitXY" android:layout_alignTop="@+id/call_type_icon" android:layout_alignBottom="@+id/call_type_icon" android:contentDescription="@string/image_content_description" android:layout_marginStart="@dimen/contact_detail_calllog_item_senond_distance" android:layout_toEndOf="@+id/call_type_icon" />
                <ImageView android:id="@+id/encrypt_call" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/encrypt_call_log_list_icon" android:scaleType="fitXY" android:layout_alignTop="@+id/call_type_icon" android:layout_alignBottom="@+id/call_type_icon" android:contentDescription="@string/image_content_description" android:layout_marginStart="@dimen/contact_detail_calllog_item_senond_distance" android:layout_toEndOf="@+id/call_type_image" />
                <ImageView android:id="@+id/call_record" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:src="@drawable/call_record" android:contentDescription="@string/call_record_description" android:layout_marginStart="@dimen/contact_detail_calllog_item_senond_distance" android:layout_toEndOf="@+id/encrypt_call" />
            </RelativeLayout>
            <TextView android:id="@+id/call_espace_text" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/name_escape" android:singleLine="true" android:layout_centerVertical="true" android:layout_marginStart="@dimen/contact_detail_calllog_item_senond_distance" android:layout_toEndOf="@+id/call_icon" style="@style/contact_calllog_detail_sub_text_style" />
            <TextView android:id="@+id/call_number_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" android:layout_centerVertical="true" android:layout_marginStart="@dimen/contact_detail_calllog_item_senond_distance" android:layout_toEndOf="@+id/call_espace_text" style="@style/contact_calllog_detail_sub_text_style" />
            <TextView android:id="@+id/ring_times" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" android:layout_alignWithParentIfMissing="true" android:layout_toStartOf="@+id/call_type_text" style="@style/contact_calllog_detail_sub_text_style" />
            <TextView android:id="@+id/call_type_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" android:layout_toStartOf="@+id/duration" style="@style/contact_calllog_detail_sub_text_style" />
            <TextView android:id="@+id/duration" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerVertical="true" android:layout_alignParentEnd="true" style="@style/contact_calllog_detail_sub_text_style" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
