<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/androidhwext" androidhwext:key="pref_key_root">
    <PreferenceCategory androidhwext:layout="@layout/preference_category" androidhwext:title="@string/rcs_message" androidhwext:key="pref_key_im_settings">
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/im_enable_title" androidhwext:key="pref_key_im" androidhwext:summary="@string/im_enable_detail" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/im_enable" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:key="divider_pref_key_im" androidhwext:preferenceID="@+id/divider_im" />
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/im_enable_delivery_report_title" androidhwext:key="pref_key_im_enable_delivery_report" androidhwext:summary="@string/im_enable_delivery_report_detail" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/im_enable_delivery_report" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:key="divider_pref_key_im_enable_delivery_report" androidhwext:preferenceID="@+id/divider_im_enable_delivery_report" />
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/im_enable_display_report_title" androidhwext:key="pref_key_im_enable_display_report" androidhwext:summary="@string/im_enable_display_report_detail" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/im_enable_display_report" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:preferenceID="@+id/list_divider_id" />
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/group_message_enable_delivery_report_title" androidhwext:key="pref_key_group_message_enable_delivery_report" androidhwext:summary="@string/group_message_enable_delivery_report_detail" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/group_message_enable_delivery_report" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:key="divider_pref_key_group_message_enable_delivery_report" androidhwext:preferenceID="@+id/divider_group_message_enable_delivery_report" />
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/auto_accept_group_invite_title" androidhwext:key="pref_key_group_invite_accept" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/auto_accept_group_invite" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:key="divider_pref_key_group_invite_accept" androidhwext:preferenceID="@+id/group_invite_accept" />
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/im_enable_thread_mix_display_title" androidhwext:key="pref_key_im_enable_thread_mix_display" androidhwext:summary="@string/im_enable_thread_mix_display_detail" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/im_enable_thread_mix_display" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:key="divider_pref_key_im_enable_thread_mix_display" androidhwext:preferenceID="@+id/divider_im_enable_thread_mix_display" />
        <Preference androidhwext:layout="@layout/rcs_mms_preference_nick_name_cust" androidhwext:title="@string/nickname_title" androidhwext:key="pref_key_mms_rcs_nick_name" androidhwext:widgetLayout="@layout/mms_preference_widget_arrow" androidhwext:preferenceID="@+id/nickname" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:key="divider_pref_key_mms_rcs_nick_name" androidhwext:preferenceID="@+id/mms_rcs_nick_name" />
        <Preference androidhwext:layout="@layout/mms_preference_status" androidhwext:title="@string/auto_accept_file_title" androidhwext:key="pref_key_auto_accept_file" androidhwext:widgetLayout="@layout/mms_preference_widget_arrow" androidhwext:defaultValue="@string/mms_auto_retrieval_during_unroaming" androidhwext:preferenceID="@+id/auto_accept_file" />
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:preferenceID="@+id/pref_list_divider" />
        <com.android.mms.ui.EmuiSwitchPreference androidhwext:title="@string/show_filesize_warning_dialog_title" androidhwext:key="pref_key_crop_not_ask_me_again" androidhwext:summary="@string/rcs_file_too_big_explain" androidhwext:defaultValue="true" androidhwext:preferenceID="@+id/show_filesize_warning_dialog" />
    </PreferenceCategory>
    <com.huawei.rcs.ui.MessagePlusPreferenceCategory androidhwext:layout="@layout/preference_category" androidhwext:title="  " androidhwext:key="pref_category_about_message">
        <PreferenceScreen androidhwext:layout="@layout/mms_preference_status" androidhwext:title="@string/rcs_about_message" androidhwext:key="pref_key_message_info" androidhwext:widgetLayout="@layout/mms_preference_widget_arrow">
            <intent androidhwext:targetPackage="com.android.mms" androidhwext:action="android.intent.action.MAIN" androidhwext:targetClass="com.huawei.rcs.ui.RcsAboutMessagePagerActivity" />
        </PreferenceScreen>
        <Preference androidhwext:layout="@layout/listdivider" androidhwext:preferenceID="@+id/category_about_message" />
    </com.huawei.rcs.ui.MessagePlusPreferenceCategory>
</PreferenceScreen>
