<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:background="@color/duoqu_split_line_color" android:layout_width="match_parent" android:layout_height="UNKNOWN_DATA_0x201" />
        <RelativeLayout android:id="@+id/duoqu_webview" android:visibility="visible" android:layout_width="match_parent" android:layout_height="match_parent" />
    </LinearLayout>
    <RelativeLayout android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:id="@+id/duoqu_progressbar" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_centerInParent="true">
        <ProgressBar android:id="@+id/duoqu_bar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:indeterminateOnly="true" android:layout_centerHorizontal="true" />
        <TextView android:textSize="13dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/duoqu_loading" android:layout_below="@+id/duoqu_bar" android:layout_centerHorizontal="true" />
    </RelativeLayout>
    <RelativeLayout android:id="@+id/duoqu_menu" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentEnd="true">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" />
    </RelativeLayout>
    <RelativeLayout android:id="@+id/duoqu_error_page" android:background="@color/duoqu_web_network_fail_bg_color" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"
        <RelativeLayout android:id="@+id/duoqu_network_setting" android:background="@color/duoqu_setting_bg_color" android:layout_width="wrap_content" android:layout_height="UNKNOWN_DATA_0x3001">
            <ImageView android:id="@+id/duoqu_img_woring" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/duoqu_nonetwork" android:layout_centerVertical="true" android:layout_marginStart="16dp" android:layout_alignParentStart="true" />
            <TextView android:textSize="15dp" android:textColor="@color/duoqu_web_network_fail_text_color" android:id="@+id/duoqu_error_lable1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="8dp" android:text="@string/duoqu_web_network_fail" android:layout_centerVertical="true" android:layout_toEndOf="@+id/duoqu_img_woring" />
            <TextView android:textSize="13dp" android:textColor="@color/duoqu_web_network_setting_text_color" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/duoqu_web_network_setting" android:layout_centerVertical="true" android:paddingEnd="32dp" android:layout_alignParentEnd="true" />
            <ImageView android:id="@+id/duoqu_img_next" android:paddingRight="16dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/duoqu_networksetting" android:layout_centerVertical="true" android:paddingStart="5dp" android:layout_alignParentEnd="true" />
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
